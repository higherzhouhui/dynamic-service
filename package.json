{
  "scripts": {
    "init": "INIT=1 node ./src/app.js",
    "init:win": "set INIT=1 && set NODE_ENV=development && node ./src/app.js",
    "start": "nodemon ./src/app.js --trace-deprecation NODE_OPTIONS=--max-old-space-size=5000",
    "dev": "set NODE_ENV=development && nodemon ./src/app.js",
    "dev:linux": "NODE_ENV=development nodemon ./src/app.js",
    "pm2": "pm2 start ./src/app.js --name 'service-dynamic'"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^7.4.1",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.5",
    "nodemon": "^3.1.9",
    "sequelize": "6.37.5",
    "urlencode": "^2.0.0"
  }
